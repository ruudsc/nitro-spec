{
  "name": "nitro-spec",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "nx": {
    "tags": [
      "library"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/compiler/factories/vite.d.ts",
      "require": "./dist/compiler/factories/vite.cjs",
      "import": "./dist/compiler/factories/vite.js"
    },
    "./webpack": {
      "types": "./dist/compiler/factories/webpack.d.ts",
      "require": "./dist/compiler/factories/webpack.cjs",
      "import": "./dist/compiler/factories/webpack.js"
    },
    "./rollup": {
      "types": "./dist/compiler/factories/rollup.d.ts",
      "require": "./dist/compiler/factories/rollup.cjs",
      "import": "./dist/compiler/factories/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/compiler/factories/esbuild.d.ts",
      "require": "./dist/compiler/factories/esbuild.cjs",
      "import": "./dist/compiler/factories/esbuild.js"
    },
    "./nuxt": {
      "types": "./dist/compiler/factories/nuxt.d.ts",
      "require": "./dist/compiler/factories/nuxt.cjs",
      "import": "./dist/compiler/factories/nuxt.js"
    },
    "./types": {
      "import": "./dist/types.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .nuxt .output node_modules",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts"
  },
  "peerDependencies": {
    "recast": "0.23.11",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "acorn": "^8.14.1",
    "consola": "^3.4.0",
    "esbuild": "*",
    "h3": "1.15.4",
    "nitropack": "^2.12.4",
    "pathe": "^2.0.3",
    "pkg-types": "^2.2.0",
    "rollup": "^3.29.5",
    "unimport": "^4.2.0",
    "vite": ">=3",
    "webpack": "^4 || ^5",
    "zod": "catalog:"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "catalog:",
    "escodegen": "^2.1.0",
    "unplugin": "^2.1.2",
    "yaml": "2.7.0"
  },
  "devDependencies": {
    "@nuxt/kit": "^4.0.2",
    "@nuxt/schema": "^4.0.2",
    "@types/escodegen": "^0.0.10",
    "@types/node": "^22.13.1",
    "acorn-walk": "^8.3.4",
    "bumpp": "^10.2.2",
    "eslint": "^9.32.0",
    "nodemon": "^3.1.10",
    "openapi3-ts": "^4.5.0",
    "prettier": "^3.6.2",
    "rollup": "^4.46.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "catalog:",
    "vite": "^7.0.6",
    "vitest": "^3.2.4",
    "webpack": "^5.101.0"
  }
}
