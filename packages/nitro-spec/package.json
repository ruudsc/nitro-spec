{
  "name": "nitro-spec",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.3.0",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/compiler/factories/vite.d.ts",
      "import": "./dist/compiler/factories/vite.js"
    },
    "./webpack": {
      "types": "./dist/compiler/factories/webpack.d.ts",
      "import": "./dist/compiler/factories/webpack.js"
    },
    "./rollup": {
      "types": "./dist/compiler/factories/rollup.d.ts",
      "import": "./dist/compiler/factories/rollup.js"
    },
    "./esbuild": {
      "types": "./dist/compiler/factories/esbuild.d.ts",
      "import": "./dist/compiler/factories/esbuild.js"
    },
    "./nuxt": {
      "types": "./dist/compiler/factories/nuxt.d.ts",
      "import": "./dist/compiler/factories/nuxt.js"
    },
    "./types": {
      "import": "./dist/types.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .nuxt .output node_modules",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts"
  },
  "peerDependencies": {
    "h3": "1.15.1",
    "nitropack": "^2.11.5",
    "acorn": "^8.14.1",
    "consola": "^3.4.0",
    "jiti": "^2.4.2",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "unimport": "^4.1.2",
    "zod": "catalog:",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "unplugin": "^2.1.2",
    "yaml": "2.7.0"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "@nuxt/kit": "^3.15.4",
    "@nuxt/schema": "^3.15.4",
    "@types/node": "^22.13.1",
    "bumpp": "^10.0.2",
    "eslint": "^9.20.0",
    "nodemon": "^3.1.9",
    "rollup": "^4.34.6",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "catalog:",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "webpack": "^5.97.1"
  }
}
